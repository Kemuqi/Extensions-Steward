{
  "manifest_version": 2,
  "name": "__MSG_ext_name__",
  "version": "3.0.0",
  "default_locale": "en",
  "description": "__MSG_ext_desc__",
  "icons": {
    "32": "icons/32.png",
    "64": "icons/64.png",
    "128": "icons/128.png"
  },
  "browser_action": {
    "default_popup": "popup.html"
  },
  "background": {
    "page": "background.html",
    "persistent": true
  },
  "homepage_url": "https://github.com/Aqours/Extensions-Steward",
  "minimum_chrome_version": "66",
  "omnibox": {
    "keyword": "ems"
  },
  "options_ui": {
    "page": "options.html",
    "chrome_style": true
  },
  "permissions": [
    "contextMenus",
    "management",
    "storage",
    "tabs"
  ]
}
